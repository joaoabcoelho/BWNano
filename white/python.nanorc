## Here is an example for Python.
##
syntax "python" "\.py$"
icolor blue "def [0-9A-Z_]+"
color magenta "\<(and|assert|class|def|elif|else|exec|finally|for|from|global|if|import|in|is|lambda|map|not|or|pass|print|raise|while)\>"
color blue "\<(del|except|try)\>"
color yellow "\<(break|continue|return)\>"
color green "['][^']*[^\\][']" "[']{3}.*[^\\][']{3}"
color green "["][^"]*[^\\]["]" "["]{3}.*[^\\]["]{3}"
color red start=""""[^"]" end=""""" start="'''[^']" end="'''"
color red "#.*$"

